    \u043D\u0430\u0441\u043A\u043E\u043B\u044C\u043A\u043E \u0431\u044B\u0441\u0442\u0440\u043E
    \u0438\u043C\u0435\u044E\u0449\u0438\u0435\u0441\u044F \u0430\u043A\u0442\u0438\u0432\u044B
    \u043C\u043E\u0433\u0443\u0442 \u0431\u044B\u0442\u044C \u043F\u0440\u043E\u0434\u0430\u043D\u044B
    \u043F\u043E \u0440\u044B\u043D\u043E\u0447\u043D\u043E\u0439 \u0438\u043B\u0438
    \u0431\u043B\u0438\u0437\u043A\u043E\u0439 \u043A \u043D\u0435\u0439 \u0446\u0435\u043D\u0435,
